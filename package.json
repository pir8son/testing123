{
  "name": "swipe-to-recipe-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "web:dev": "npm --prefix apps/web run dev",
    "mobile:start": "npm --prefix apps/mobile run start --",
    "mobile:ios": "npm --prefix apps/mobile run ios",
    "mobile:android": "npm --prefix apps/mobile run android",
    "mobile:doctor": "npm --prefix apps/mobile run doctor",
    "dom:lint": "node scripts/dom-lint.mjs",
    "typecheck": "npm --prefix packages/shared run typecheck && npm --prefix apps/mobile run typecheck && npm --prefix apps/web run typecheck",
    "deps:react": "npm ls react react-native"
  }
}
